<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".end">

    <TextView
            android:text="おわり"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/textView23" android:textAppearance="@style/TextAppearance.AppCompat.Display3"
            android:layout_marginTop="150dp" app:layout_constraintTop_toTopOf="parent"/>
    <TextView
            android:text="あなたは"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/hyouzi" android:layout_marginTop="100dp"
            app:layout_constraintTop_toBottomOf="@+id/textView23" android:gravity="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Medium"/>
    <TextView
            android:text="戻る"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/endbutton" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginBottom="24dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1"
            android:background="@drawable/frame_layout" android:layout_marginStart="24dp"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editText"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="98dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="100dp" android:layout_marginTop="32dp"
            app:layout_constraintTop_toBottomOf="@+id/hyouzi" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/save" android:gravity="center" android:hint="名前を教えてね?"/>
    <TextView
            android:text="記録する"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:layout_marginEnd="24dp"
            app:layout_constraintEnd_toEndOf="parent" android:id="@+id/save"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="24dp"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1"
            android:background="@drawable/frame_layout"/>
    <TextView
            android:text="名前を記録するとランキングにのるかも"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/textView24" app:layout_constraintTop_toBottomOf="@+id/hyouzi"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/editText"/>
</androidx.constraintlayout.widget.ConstraintLayout>