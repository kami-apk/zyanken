<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".zyanken_anime">
    <ImageView
            android:layout_width="300dp"
            android:layout_height="300dp" tools:srcCompat="@tools:sample/avatars"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:id="@+id/image"
            app:layout_constraintHorizontal_bias="0.495" android:scaleType="centerCrop"
            android:layout_marginTop="64dp"
            app:layout_constraintTop_toTopOf="parent" android:background="@drawable/kakomi"/>
    <TextView
            android:text="勝て"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/textView15" android:textAppearance="@style/TextAppearance.AppCompat.Display2"
            android:layout_marginTop="64dp" app:layout_constraintTop_toBottomOf="@+id/image"/>
    <ImageView
            android:layout_width="100dp"
            android:layout_height="100dp" app:srcCompat="@drawable/choki"
            android:id="@+id/choki" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintEnd_toStartOf="@+id/gu" android:layout_marginEnd="8dp" android:layout_marginTop="64dp"
            app:layout_constraintTop_toBottomOf="@+id/textView15"/>
    <ImageView
            android:layout_width="100dp"
            android:layout_height="100dp" app:srcCompat="@drawable/gu"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/gu" app:layout_constraintHorizontal_bias="0.479" android:layout_marginBottom="32dp"
            app:layout_constraintBottom_toBottomOf="parent"/>
    <ImageView
            android:layout_width="100dp"
            android:layout_height="100dp" app:srcCompat="@drawable/pa"
            android:id="@+id/pa" app:layout_constraintStart_toEndOf="@+id/gu" android:layout_marginStart="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp" android:layout_marginTop="64dp"
            app:layout_constraintTop_toBottomOf="@+id/textView15"/>
</androidx.constraintlayout.widget.ConstraintLayout>