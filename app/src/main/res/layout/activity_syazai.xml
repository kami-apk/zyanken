<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".syazai" android:id="@+id/open">

    <TextView
            android:text="クレジット"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/textView17" android:textAppearance="@style/TextAppearance.AppCompat.Display3"
            android:layout_marginTop="150dp" app:layout_constraintTop_toTopOf="parent"/>
    <TextView
            android:text="このゲームを遊んでくれてありがとう。\n制作者：かみ\nTwitter：@kami_apk"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:id="@+id/textView19" android:gravity="center"/>
    <TextView
            android:text="仕様素材\nシルエットコレクション\nイラストや\nロゴメーカー"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/textView21" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView19" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>